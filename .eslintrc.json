{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        // "plugin:react/recommended",
        // "airbnb/hooks",
        // "airbnb",
        "plugin:react/jsx-runtime"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [
            0
        ]
    }
}